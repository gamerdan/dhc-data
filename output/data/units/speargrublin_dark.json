  "Skills": {
    "FirstSkill": {
      "id": "speargrublin_poisonedSpear",
      "name": {
        "en": "Poisoned Spear",
        "fr": "Lance empoisonnée",
        "de": "Vergifteter Speer",
        "es": "Lanza Envenenada"
      },
      "description": {
        "en": "Hurls his spear at a location, damaging enemies in a small area. Has a chance to inflict <font color='{debuff_red}'>{icon.mortal}&nbsp;MORTAL WOUNDS</font>.",
        "fr": "Jette sa lance pour infliger des dégâts aux ennemis dans une petite zone. Peut causer <font color='{debuff_red}'>{icon.mortal}&nbsp;TRAUMATISME</font>.",
        "de": "Schleudert seinen Speer auf eine Stelle und fügt Feinden in einem kleinen Gebiet Schaden zu. Fügt Schaden zu mit einer Chance, <font color='{debuff_red}'>{icon.mortal}&nbsp;TÖDLICHE WUNDEN</font> zu wirken.",
        "es": "Lanza su jabalina a una ubicación y daña a los enemigos en una pequeña zona. Tiene probabilidad de infligir <font color='{debuff_red}'>{icon.mortal}&nbsp;HERIDAS MORTALES</font>."
      }
    },
    "SecondSkill": {
      "id": "speargrublin_bigBadCharge",
      "name": {
        "en": "Big Bad Charge",
        "fr": "Gros grabuge grublin",
        "de": "Großer böser Angriff",
        "es": "Carga Feroz"
      },
      "description": {
        "en": "Charges forward in a straight line until he hits an enemy. Deals damage and inflicts <font color='{debuff_red}'>{icon.ongoing}&nbsp;ONGOING DAMAGE</font> on that enemy.",
        "fr": "Fonce tête baissée jusqu'à percuter un ennemi. Inflige des dégâts ainsi qu'<font color='{debuff_red}'>{icon.ongoing}&nbsp;USURE</font> à cet ennemi.",
        "de": "Greift in einer geraden Linie nach vorn an, bis er einen Feind trifft. Fügt Schaden zu und wirkt <font color='{debuff_red}'>{icon.ongoing}&nbsp;ANDAUERNDEN SCHADEN</font>.",
        "es": "Carga hacia adelante en línea recta hasta que golpea a un enemigo. Daña e inflige <font color='{debuff_red}'>{icon.ongoing}&nbsp;DAÑO CONTINUADO</font> al enemigo."
      }
    },
    "AutoAttackSkill": {
      "id": "speargrublin_pokePoke",
      "name": {
        "en": "Poke Poke!",
        "fr": "Pichenette",
        "de": "Pieks pieks!",
        "es": "Pincho Moruno"
      },
      "description": {
        "en": "Attacks enemies with his spear with a chance to <font color='{buff_blue}'>{icon.raise_crit}&nbsp;RAISE CRIT</font>.",
        "fr": "Utilise sa lance pour infliger des dégâts aux ennemis et peut obtenir <font color='{buff_blue}'>{icon.raise_crit}&nbsp;CRIT+</font>.",
        "de": "Greift Feinde mit seinem Speer an mit einer Chance auf <font color='{buff_blue}'>{icon.raise_crit}&nbsp;KRIT. CHANCE ERHÖHEN</font>.",
        "es": "Ataca a los enemigos con su lanza con probabilidad de <font color='{buff_blue}'>{icon.raise_crit}&nbsp;AUMENTAR CRÍT</font>."
      }
    }
  },
