  "Skills": {
    "FirstSkill": {
      "id": "speargrublin_javelinToss",
      "name": {
        "en": "Javelin Toss",
        "fr": "Lancer de javelot",
        "de": "Speerwurf",
        "es": "Lanzamiento de Jabalina"
      },
      "description": {
        "en": "Hurls his spear at a location, damaging enemies in a small area. Has a chance to <font color='{debuff_red}'>{icon.immobilize}&nbsp;IMMOBILIZE</font>.",
        "fr": "Jette sa lance pour infliger des dégâts aux ennemis dans une petite zone. Peut causer <font color='{debuff_red}'>{icon.immobilize}&nbsp;ENTRAVE</font>.",
        "de": "Schleudert seinen Speer auf eine Stelle und fügt Feinden in einem kleinen Gebiet Schaden zu. Hat eine Chance, <font color='{debuff_red}'>{icon.immobilize}&nbsp;BEWEGUNGSUNFÄHIG</font> zu machen.",
        "es": "Lanza su jabalina a una ubicación y daña a los enemigos en una pequeña zona. Tiene probabilidad de <font color='{debuff_red}'>{icon.immobilize}&nbsp;PARALIZAR</font>."
      }
    },
    "SecondSkill": {
      "id": "speargrublin_bigBadBlitz",
      "name": {
        "en": "Big Bad Blitz",
        "fr": "Grosse gamelle grubline",
        "de": "Großer böser Blitz",
        "es": "Ataque Feroz"
      },
      "description": {
        "en": "Charges forward in a straight line until he hits an enemy. Deals damage, <font color='{debuff_red}'>{icon.lower_def}&nbsp;LOWERS DEF</font>, and <font color='{debuff_red}'>{icon.mark}&nbsp;MARKS</font> that enemy. ",
        "fr": "Fonce tête baissée jusqu'à percuter un ennemi. Inflige des dégâts, <font color='{debuff_red}'>{icon.lower_def}&nbsp;DÉF-</font> et <font color='{debuff_red}'>{icon.mark}&nbsp;MARQUAGE</font> à cet ennemi.",
        "de": "Greift in einer geraden Linie nach vorn an, bis er einen Feind trifft. Fügt Schaden zu, <font color='{debuff_red}'>{icon.lower_def}&nbsp;VERRINGERT VTG.</font> und <font color='{debuff_red}'>{icon.mark}&nbsp;MARKIERT</font> diesen Feind.",
        "es": "Carga hacia adelante en línea recta hasta que golpea a un enemigo. Daña, <font color='{debuff_red}'>{icon.lower_def}&nbsp;REDUCE DEF</font> y<font color='{debuff_red}'>{icon.mark}&nbsp;MARCA</font> al enemigo."
      }
    },
    "AutoAttackSkill": {
      "id": "speargrublin_pokePoke",
      "name": {
        "en": "Poke Poke!",
        "fr": "Pichenette",
        "de": "Pieks pieks!",
        "es": "Pincho Moruno"
      },
      "description": {
        "en": "Attacks enemies with his spear with a chance to <font color='{buff_blue}'>{icon.raise_crit}&nbsp;RAISE CRIT</font>.",
        "fr": "Utilise sa lance pour infliger des dégâts aux ennemis et peut obtenir <font color='{buff_blue}'>{icon.raise_crit}&nbsp;CRIT+</font>.",
        "de": "Greift Feinde mit seinem Speer an mit einer Chance auf <font color='{buff_blue}'>{icon.raise_crit}&nbsp;KRIT. CHANCE ERHÖHEN</font>.",
        "es": "Ataca a los enemigos con su lanza con probabilidad de <font color='{buff_blue}'>{icon.raise_crit}&nbsp;AUMENTAR CRÍT</font>."
      }
    }
