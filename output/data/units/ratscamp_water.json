  "Skills": {
    "FirstSkill": {
      "id": "ratscamp_exploitWeakness",
      "name": {
        "en": "Exploit Weakness",
        "fr": "Exploitation de faiblesse",
        "de": "Schwäche ausnutzen",
        "es": "Debilidad Explotada"
      },
      "description": {
        "en": "Leaps at a target enemy, dealing damage with his claws.<br><font color='{menu_highlight_white}'>Bonus damage based on the number of debuffs on the enemy.</font>",
        "fr": "Bondit sur l'ennemi pour lui infliger des dégâts avec ses griffes.<br><font color='{menu_highlight_white}'>Inflige des dégâts supplémentaires en fonction du nombre de debuffs de l'ennemi.</font>",
        "de": "Springt auf ein feindliches Ziel zu und fügt mit seinen Klauen Schaden zu.<br><font color='{menu_highlight_white}'>Bonus-Schaden, basierend auf der Anzahl an Schwächungszaubern auf dem Feind.</font>",
        "es": "Se abalanza sobre el enemigo infligiendo daño con sus garras.<br><font color='{menu_highlight_white}'>El daño adicional depende del número de efectos negativos del enemigo.</font>"
      }
    },
    "SecondSkill": {
      "id": "ratscamp_sandSpray",
      "name": {
        "en": "Sand Spray",
        "fr": "Projection de sable",
        "de": "Sandsprüher",
        "es": "Rocío de Arena"
      },
      "description": {
        "en": "Kicks sand into the eyes of his foes. Deals damage and <font color='{debuff_red}'>{icon.lower_atk}&nbsp;LOWERS ATK</font>.",
        "fr": "Projette du sable dans les yeux de ses ennemis. Inflige des dégâts et <font color='{debuff_red}'>{icon.lower_atk}&nbsp;ATT-</font>.",
        "de": "Schleudert seinen Feinden Sand in die Augen. Fügt Feinden Schaden zu und <font color='{debuff_red}'>{icon.lower_atk}&nbsp;VERRINGERT ANG.</font>",
        "es": "Lanza arena a los ojos de sus enemigos. Inflige daño y <font color='{debuff_red}'>{icon.lower_atk}&nbsp;REDUCE ATQ</font>."
      }
    },
    "AutoAttackSkill": {
      "id": "ratscamp_doubleTrouble",
      "name": {
        "en": "Double Trouble",
        "fr": "Double attaque",
        "de": "Doppelter Ärger",
        "es": "Dos en Uno"
      },
      "description": {
        "en": "Rapidly slashes enemies with his claws, damaging them two times with a single attack.",
        "fr": "Lacère rapidement les ennemis avec ses griffes, leur infligeant des dégâts deux fois en une seule attaque.",
        "de": "Schlitzt Feinde blitzschnell mit seinen Klauen auf, wobei er ihnen mit einem einzelnen Angriff zwei Mal Schaden zufügt.",
        "es": "Raja a los enemigos rápidamente con sus garras, dañándolos dos veces con un solo ataque."
      }
    }
  },
