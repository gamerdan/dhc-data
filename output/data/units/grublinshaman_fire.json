  "Skills": {
    "FirstSkill": {
      "id": "grublinshaman_jinx",
      "name": {
        "en": "Jinx",
        "fr": "Porte-malheur",
        "de": "Jinx",
        "es": "Maldición"
      },
      "description": {
        "en": "Throws an evil curse at target enemy that bounces between enemies, hitting up to {SkillCustomParameters.NumberOfBounce} targets. Damages enemies and <font color='{debuff_red}'>{icon.lower_atk_spd}&nbsp;LOWERS ATK SPD</font>.",
        "fr": "Lance sur un ennemi visé une malédiction capable de ricocher au maximum sur {SkillCustomParameters.NumberOfBounce} ennemis. Inflige des dégâts et <font color='{debuff_red}'>{icon.lower_atk_spd}&nbsp;VIT ATT-</font> aux ennemis.",
        "de": "Wirft einen bösen Fluch auf ein feindliches Ziel, der zwischen den Feinden hin und her springt und bis zu {SkillCustomParameters.NumberOfBounce} Feinde trifft. Fügt Feinden Schaden zu und <font color='{debuff_red}'>{icon.lower_atk_spd}&nbsp;VERRINGERT ANG.-TMP.</font>",
        "es": "Lanza una maldición maligna que rebota entre los enemigos, golpeando a un máximo de {SkillCustomParameters.NumberOfBounce} objetivos. Daña a los enemigos y <font color='{debuff_red}'>{icon.lower_atk_spd}&nbsp;REDUCE VEL ATQ</font>."
      }
    },
    "AutoAttackSkill": {
      "id": "grublinshaman_dazzle",
      "name": {
        "en": "Dazzle",
        "fr": "Éclat",
        "de": "Blendung",
        "es": "Resplandor"
      },
      "description": {
        "en": "Fires a glowing ball of magic to damage an enemy from a distance with a chance to <font color='{debuff_red}'>{icon.disorient}&nbsp;DISORIENT</font>.",
        "fr": "Tire une boule de magie brillante pour infliger des dégâts pouvant causer <font color='{debuff_red}'>{icon.disorient}&nbsp;CONFUSION</font> à un ennemi à distance.",
        "de": "Feuert einen glühenden Zauberball, um einem Feind aus der Entfernung Schaden zuzufügen, mit einer Chance auf <font color='{debuff_red}'>{icon.disorient}&nbsp;DESORIENTIEREN</font>.",
        "es": "Lanza una bola mágica resplandeciente que inflige daño a distancia a un enemigo y tiene probabilidad de <font color='{debuff_red}'>{icon.disorient}&nbsp;DESORIENTAR</font>."
      }
    }
