  "Skills": {
    "FirstSkill": {
      "id": "treadrobot_unstoppableAdvance",
      "name": {
        "en": "Unstoppable Advance",
        "fr": "Avancée inexorable",
        "de": "Unaufhaltsamer Vorsprung",
        "es": "Avance Imparable"
      },
      "description": {
        "en": "Gains the <font color='{buff_blue}'>{icon.invulnerability}&nbsp;INVULNERABILITY</font> buff and then charges in a line, damaging and pushing back the first enemy it hits.",
        "fr": "Gagne <font color='{buff_blue}'>{icon.invulnerability}&nbsp;INVULNÉRABILITÉ</font> et charge droit devant lui, infligeant des dégâts et repoussant le premier ennemi rencontré.",
        "de": "Erhält den <font color='{buff_blue}'>{icon.invulnerability}&nbsp;UNVERWUNDBARKEIT</font>-Bonus und greift dann in einer Linie an, fügt dem ersten getroffenen Feind Schaden zu und schiebt ihn weg.",
        "es": "Obtiene el efecto positivo <font color='{buff_blue}'>{icon.invulnerability}&nbsp;INVULNERABILIDAD</font> y después embiste en línea recta, dañando y empujando al primer enemigo que golpee."
      }
    },
    "SecondSkill": {
      "id": "treadrobot_laserFlash",
      "name": {
        "en": "Laser Flash",
        "fr": "Flash de laser",
        "de": "Laserblitz",
        "es": "Destello de Láser"
      },
      "description": {
        "en": "Spins around, carving a circle in the ground with a laser. Damages enemies and <font color='{debuff_red}'>{icon.blind}&nbsp;BLINDS</font>.",
        "fr": "Tourbillonne et creuse un cercle dans le sol avec un laser. Inflige des dégâts aux ennemis et <font color='{debuff_red}'>{icon.blind}&nbsp;CÉCITÉ</font>.",
        "de": "Dreht sich um sich selbst und schneidet mit einem Laser einen Kreis in den Boden. Fügt Feinden Schaden zu und <font color='{debuff_red}'>{icon.blind}&nbsp;BLENDET</font> sie.",
        "es": "Gira creando un círculo en el suelo con un láser. Daña a los enemigos y los <font color='{debuff_red}'>{icon.blind}&nbsp;CIEGA</font>."
      }
    },
    "AutoAttackSkill": {
      "id": "treadrobot_crushingPincer",
      "name": {
        "en": "Crushing Pincer",
        "fr": "Tenaille dévastatrice",
        "de": "Zerquetschende Scheren",
        "es": "Tenazas Devastadoras"
      },
      "description": {
        "en": "Attacks enemies with its claws, with a chance that it <font color='{debuff_red}'>{icon.lower_atk}&nbsp;LOWERS ATK</font>.",
        "fr": "Attaque les ennemis avec ses griffes, pouvant infliger <font color='{debuff_red}'>{icon.lower_atk}&nbsp;ATT-</font>.",
        "de": "Greift Feinde mit seinen Klauen an, mit einer Chance auf <font color='{debuff_red}'>{icon.lower_atk}&nbsp;ANG. VERRINGERT</font>.",
        "es": "Ataca a sus enemigos con sus pinzas con probabilidad de <font color='{debuff_red}'>{icon.lower_atk}&nbsp;REDUCIR ATQ</font>."
      }
