  "Skills": {
    "FirstSkill": {
      "id": "tridentlizard_spearStorm",
      "name": {
        "en": "Spear Storm",
        "fr": "Tempête de lance",
        "de": "Speersturm",
        "es": "Tormenta de Lanza"
      },
      "description": {
        "en": "Thrusts his spear with incredible speed, dealing damage to enemies in a cone.<br><font color='{menu_highlight_white}'>Deals more damage if it hits {SkillCustomParameters.targetTreshold} or more enemies at once.</font>",
        "fr": "Plonge sa lance à une vitesse incroyable, infligeant des dégâts aux ennemis situés dans le cône d'effet.<br><font color='{menu_highlight_white}'>Inflige des dégâts supplémentaires si {SkillCustomParameters.targetTreshold} ennemis ou plus sont touchés à la fois.</font>",
        "de": "Stößt seinen Speer mit unglaublicher Geschwindigkeit und fügt Feinden innerhalb eines Kegels Schaden zu.<br><font color='{menu_highlight_white}'>Fügt mehr Schaden zu, wenn er {SkillCustomParameters.targetTreshold} oder mehr Feinde mit einem Mal trifft.</font>",
        "es": "Embiste con su lanza a gran velocidad, dañando a los enemigos en un cono.<br><font color='{menu_highlight_white}'>Inflige más daño si golpea a {SkillCustomParameters.targetTreshold} o más enemigos a la vez.</font>"
      }
    },
    "SecondSkill": {
      "id": "tridentlizard_sturdyScales",
      "name": {
        "en": "Sturdy Scales",
        "fr": "Écailles robustes",
        "de": "Robuste Schuppen",
        "es": "Placas Resistentes"
      },
      "description": {
        "en": "When receiving damage from a crit, has a chance to reduce that damage by {~%SkillCustomParameters.reducedDamageRatio}.",
        "fr": "Peut réduire les dégâts critiques subis de {~%SkillCustomParameters.reducedDamageRatio}.",
        "de": "Wenn durch einen krit. Treffer Schaden erlitten wird, hat er die Chance, den Schaden um {~%SkillCustomParameters.reducedDamageRatio} zu reduzieren.",
        "es": "Al recibir daño crítico, existe la posibilidad de reducirlo un {~%SkillCustomParameters.reducedDamageRatio}."
      }
    },
    "AutoAttackSkill": {
      "id": "tridentlizard_frenziedJab",
      "name": {
        "en": "Frenzied Jab",
        "fr": "Assaut frénétique",
        "de": "Wilder Stoß",
        "es": "Golpe Furibundo"
      },
      "description": {
        "en": "Attacks enemies with his trident.<br><font color='{menu_highlight_white}'>Deals more damage as current HP% decreases</font>.",
        "fr": "Attaque les ennemis avec son trident.<br><font color='{menu_highlight_white}'>Inflige plus de dégâts à mesure que le % de HP actuel diminue.</font>",
        "de": "Greift Feinde mit seinem Dreizack an.<br><font color='{menu_highlight_white}'>Fügt mehr Schaden zu, als die derzeitigen GP-% sinken.</font>",
        "es": "Ataca a los enemigos con su tridente.<br><font color='{menu_highlight_white}'>Inflige más daño a medida que baja el porcentaje de PV restantes</font>."
      }
    }
  },
